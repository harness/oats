{{description}}
export function {{hookName}}(
  props: {{fetcherPropsName}},
  options?: Omit<UseQueryOptions<{{fetcherPropsName}}ReturnValue, {{errorResponseName}}>, 'queryKey' | 'queryFn'>
) {
  return useQuery<{{fetcherPropsName}}ReturnValue, {{errorResponseName}}>(
    [
      {{operation.operationId | json}}
      {% if pathParamsCode -%}
      {% for name in pathParamsNamesList -%}
      , props{{name | property_accessor}}
      {%- endfor %}
    {%- endif -%}
    {% if queryParamsCode -%}
    , props.queryParams
    {%- endif -%}
    {% if requestBodyCode %}
    , props.body
    {%- endif %}
    ],
    ({ signal }) => {{fetcherName}}({ ...props, signal }),
    options
  );
}
